package engine.ui;

import java.awt.Rectangle;

public class Button {
	public Rectangle rect;
	public String text;
	public ButtonCallback callBack;
	
	public Button(Rectangle rect, String text, ButtonCallback callBack) {
		this.rect = rect;
		this.text = text;
		this.callBack = callBack;
	}
	
	public void render(Graphics g) {
		
	}
	
	public void tick() {
		
	}
}
